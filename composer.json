{
    "name": "normanhuth/nova-menu-order",
    "description": "Sort the resources and resources groups in the Nova menu",
    "keywords": [
        "laravel",
        "nova",
        "menu",
        "order",
        "sort"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Norman Huth",
            "email": "norman@huth.it",
            "homepage": "https://huth.it"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "laravel/framework": "^5.6|^6.0|^7.0",
        "laravel/nova": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "NormanHuth\\NovaMenuOrder\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NormanHuth\\NovaMenuOrder\\NovaMenuOrderServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
